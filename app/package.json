{
  "name": "exercise-tracker",
  "version": "0.1.0",
  "description": "Experimental project with Express and MongoDB",
  "engines": {
    "node": ">=v19.8.0"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "lint": "npx eslint src",
    "pret": "npx prettier --check src",
    "pret:fix": "npx prettier --write src",
    "format": "npm run lint && npm run pret"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "mongoose": "^6.7.0"
  },
  "repository": {
    "url": "https://github.com/kkig/express-exercise-tracker"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "express"
  ],
  "devDependencies": {
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.2.2",
    "nodemon": "^2.0.20",
    "prettier": "2.8.7",
    "supertest": "^6.3.1"
  }
}
